{
  "executionRoleArn": "arn:aws:iam::459454711632:role/flask-task-execution-role",
  "containerDefinitions":  
    [
        {
            "name": "flask",
            "image": "459454711632.dkr.ecr.us-east-1.amazonaws.com/koffeeluv-ecr_repo",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000
                }
            ]
        }
    ],
    "requiredCompatabilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "flask"
}
